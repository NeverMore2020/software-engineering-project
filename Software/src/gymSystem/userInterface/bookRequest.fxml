<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="312.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gymSystem.controller.afterLogin.BookRequestController">
   <children>
      <Text layoutX="179.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Your Request">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <TextArea fx:id="userRequest" layoutX="54.0" layoutY="55.0" prefHeight="200.0" prefWidth="390.0" promptText="Please input your request to trainer" />
      <Button layoutX="54.0" layoutY="270.0" mnemonicParsing="false" onMouseClicked="#confirmBooking" prefHeight="26.0" prefWidth="171.0" style="-fx-background-color: black;" text="Confirm" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button layoutX="272.0" layoutY="270.0" mnemonicParsing="false" onMouseClicked="#returnToBookingPage" prefHeight="26.0" prefWidth="171.0" style="-fx-background-color: white;" text="Cancel">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
   </children>
</AnchorPane>
